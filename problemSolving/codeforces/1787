#include<bits/stdc++.h>
using namespace std;
#define ll long long
#define fr first
#define sc second
#define f(i,n) for(int i=0;i<n;++i)

#define fastio                   \
    ios::sync_with_stdio(false); \
    cin.tie(0);
#define nl "\n"
void solve()
{
    ll t;
    cin>>t;
    while(t--)
    {
		ll n;
		cin>>n;
		if(n%10==0){
			 int c=0;
			 ll nm =n;
			 while(nm>1){
			 	nm = nm/10;
			 	c++;
			 }
			 cout<<c*10<<nl;
		}else{
			
		}
    }

}


int main()
{
    fastio;



    solve();
    return 0;
}

